package Exercise3;

public class FindNextPalindromicDate {
	private String day;
	private String month;
	private String year;
	
	public FindNextPalindromicDate(int day, int month, int year){
		this.day = Integer.toString(day);
		this.year = Integer.toString(year);
		this.month = Integer.toString(year);
	}
	
	public String toString(){
		String tempDay; String tempMonth; String tempYear;
		if (Integer.parseInt(day) < 10){
			tempDay = "0"+day;
		}
		if (Integer.parseInt(month) < 10){
			tempMonth
		}
		return null;
	}
}
